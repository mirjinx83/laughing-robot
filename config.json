{
    "inbounds": [
      {
        "listen": "127.0.0.1", //只监听本机，避免本机外的机器探测到下面端口。
        "port": 2023, //VLESS+SplitHTTP 监听端口
        "protocol": "vless",
        "settings": {
          "clients": [
            {
              "id": "492f8b68-81ac-4760-83d7-b095706eea88", //修改为自己的 UUID
              "email": "2023@gmail.com"
            }
          ],
          "decryption": "none"
        },
        "streamSettings": {
          "network": "splithttp",
          "splithttpSettings": {
            "path": "/"
          }
        },
        "sniffing": {
          "enabled": true,
          "destOverride": [
            "http",
            "tls"
          ]
        }
      }
    ],
    "routing": {
      "rules": [
        {
          "type": "field",
          "protocol": [
            "bittorrent"
          ],
          "outboundTag": "blocked"
        }
      ]
    },
    "outbounds": [
      {
        "protocol": "freedom",
        "settings": {}
      },
      {
        "tag": "blocked",
        "protocol": "blackhole",
        "settings": {}
      }
    ]
  }
